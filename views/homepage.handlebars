      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>
</head>
<body>
    <header>
        <h1>Fit To Go</h1>
        <h2 is="date"></h2>
    </header>

   <aside>
    <button id="b1">My Schedule</button>
    <button id="b2">My Workouts</button>
    <button id="b3">Workouts</button>
    <button id="b4">Settings</button>
  </aside>

  <section class="col-md-7">
    <p>
     <!--if there is no schedule display "You have no workouts sceduled for today! e-->
    </p>
    {{#unless schedule}}
    <h3>You have no workouts sceduled for today!</h3>
    {{/unless}}

    <!--if there is then display schedule-->
    {{#if schedule}}
    
    {{/if}}
  </section>
</body>
{{/each}}